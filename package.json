{
  "name": "grocerylist-www",
  "version": "1.0.0",
  "description": "Angular grocerylist",
  "main": "index.js",
  "scripts": {
    "dev": "concurrent --kill-others \"better-npm-run watch\" \"better-npm-run server\""
  },
  "betterScripts": {
    "watch": {
      "command": "gulp watch",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development"
      }
    },
    "server": {
      "command": "nodemon bin/server.js",
      "env": {
        "NODE_ENV": "development",
        "SERVER": true,
        "PORT": 3000
      }
    }
  },
  "author": "buchanaf@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-config-angular": "^0.4.0",
    "eslint-plugin-angular": "^0.15.0",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.2",
    "gulp-clean": "^0.3.1",
    "gulp-concat": "^2.6.0",
    "gulp-connect": "^2.3.1",
    "gulp-connect-proxy": "^0.3.1",
    "gulp-eslint": "^2.0.0",
    "gulp-inject": "^3.0.0",
    "gulp-minify-css": "^1.2.3",
    "gulp-postcss": "^6.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.1",
    "postcss-clearfix": "^1.0.0",
    "postcss-css-variables": "^0.5.1",
    "postcss-custom-media": "^5.0.1",
    "postcss-import": "^8.0.2",
    "postcss-mixins": "^4.0.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "angular": "^1.4.9",
    "angular-animate": "^1.5.0",
    "angular-aria": "^1.5.0",
    "angular-material": "^1.0.5",
    "angular-route": "^1.4.9",
    "babel-polyfill": "^6.3.14",
    "better-npm-run": "0.0.6",
    "bluebird": "^3.2.2",
    "bookshelf": "^0.9.1",
    "compression": "^1.6.1",
    "connect-redis": "^3.0.2",
    "cookie-parser": "^1.4.1",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "knex": "^0.9.0",
    "ng-dialog": "^0.6.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.0",
    "pg": "^4.4.4",
    "redis": "^2.4.2",
    "superagent": "^1.7.2"
  }
}
